<h3> EDIT THIS BLOG </h3>
<%= form_with model: @blog, method: :patch do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>

    <br>
    <%= form.label :content %>
    <%= form.text_field :content %>

    <br>
    <%= form.submit 'Update Post' %>
    <% end %>

    <h3> <%= link_to 'Return Home', blogs_path %> </h3>
    <h3> <%= link_to 'Return Post', blog_path %> </h3>
